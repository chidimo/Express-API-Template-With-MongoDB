{
  "name": "express-api-template-with-mongodb",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "babel ./src --out-dir build& node ./build/bin/www",
    "startdev": "nodemon --exec babel-node ./src/bin/www",
    "lint": "./node_modules/.bin/eslint ./src",
    "pretty": "prettier --write '**/*.{js,json}' '!node_modules/**' '!build/**' '!coverage/**' '!.nyc_output/**'",
    "postpretty": "yarn lint --fix",
    "test": "nyc --reporter=html --reporter=text --reporter=lcov mocha --timeout 20s -r @babel/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "createData": "babel-node ./src/utils/createData",
    "deleteData": "babel-node ./src/utils/deleteData"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "~4.17.3",
    "helmet": "^3.21.3",
    "http-errors": "~1.6.3",
    "mongoose": "^5.9.4",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.4.0",
    "supertest": "^4.0.2"
  }
}
